<h1>Contacts</h1>
<%= link_to "New contact", new_contact_path %>
<table class="table">
  <tr>
    <th>ID</th>
    <th>Contact Name</th>
  </tr>
  <% @contacts.each do |contact| %>
    <tr>
      <td><%= contact.id %></td>
      <td><%= link_to contact.name, contact_path(contact) %></td>
    </tr>
  <% end %>
</table>
